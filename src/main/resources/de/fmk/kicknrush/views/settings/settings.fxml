      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints vgrow="ALWAYS" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints hgrow="NEVER" />
      <ColumnConstraints hgrow="ALWAYS" />
   </columnConstraints>
   <children>
      <Label text="%settings" GridPane.columnSpan="2147483647" />
      <Label fx:id="userSettingsLabel" text="%title.user" GridPane.rowIndex="1" />
      <Label fx:id="appSettingsLabel" text="%title.app" GridPane.rowIndex="2" />
      <ScrollPane fitToWidth="true" hbarPolicy="NEVER" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.rowSpan="2147483647">
         <content>
            <VBox fx:id="settingsBox" spacing="10.0" />
         </content>
      </ScrollPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
